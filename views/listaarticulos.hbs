<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
<body style="background-color: #454A4D;">

<table class="table">
  <thead>
    <tr>
      <th class="text-white">Codigo</th>
      <th class="text-white" >Descripcion</th>
      <th class="text-white" >Precio</th>
    </tr>
  </thead>
  <tbody>
    {{#each articulos}}
    <tr class="table-dark">
       <td>{{codigo}} </td> <td>{{descripcion}}</td> <td>{{precio}}</td>
    </tr>

    {{/each}}
  </tbody>
</table>
<a  class="btn btn-primary btn-lg active" role="button" aria-pressed="true" style="font-size: 20px;" href="/">Retornar</a>
</body>
